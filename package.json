{
    "name": "node-web-server",
    "version": "1.0.0",
    "description": "wgs Node Static Web Server",
    "main": "index.js",
    "dependencies": {
        "chalk": "^2.4.2",
        "etag": "^1.8.1",
        "handlebars": "^4.1.2",
        "mime": "^2.4.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.0.0",
        "@commitlint/config-conventional": "^8.0.0",
        "babel-eslint": "^10.0.2",
        "commitizen": "^3.1.1",
        "cz-conventional-changelog": "^2.1.0",
        "eslint": "^6.0.1",
        "husky": "^3.0.0",
        "nodemon": "^1.19.1",
        "pre-commit": "^1.2.2",
        "supervisor": "^0.12.0"
    },
    "scripts": {
        "dev": "supervisor src/index.js",
        "lint": "eslint --fix .",
        "commit": "git cz"
    },
    "pre-commit": [
        "lint"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WGSHUHU/nodeWebServer.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/WGSHUHU/nodeWebServer/issues"
    },
    "homepage": "https://github.com/WGSHUHU/nodeWebServer#readme"
}
